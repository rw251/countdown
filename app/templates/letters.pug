#letters-page.letters-container.page
  .letter-grid
    - var n = 1
    while n < 4
      .tile-triplet
        - var m = 1
        while m < 4
          .letter-tile
            .tile-wrapper
              .tile 
              - m++
        - n++
  .letter-declare.hidden
    - var n = 0
    while n < 3
      .tile-triplet
        - var m = 1
        while m < 4
          .letter-tile
            .tile-wrapper
              .tile= 3*n+m
              - m++
        - n++
  #message
